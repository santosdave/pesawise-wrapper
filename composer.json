{
  "name": "santosdave/pesawise-wrapper",
  "description": "A Laravel package for integrating Pesawise payment gateway",
  "type": "library",
  "keywords": [
    "santosdave",
    "pesawise-wrapper",
    "pesawise",
    "payments"
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "orchestra/testbench": "^6.0|^7.0|^8.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Santosdave\\PesawiseWrapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Santosdave\\PesawiseWrapper\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "authors": [
    {
      "name": "santosdave",
      "email": "51959957+santosdave@users.noreply.github.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Santosdave\\PesawiseWrapper\\PesawiseProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
